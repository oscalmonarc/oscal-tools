   examples = examples + to_api_url("#{by_policy_id}")
   examples = examples + to_api_url("#{by_policy_id}detailed")
   examples = examples + to_api_url("#{by_policy_id}detailed/oscal")
   examples = examples + to_api_url("#{by_policy_id}security_control_families")
   examples = examples + to_api_url("#{by_policy_id}security_controls")
   examples = examples + "<br>"  
   the_control = SecurityControl.first   
